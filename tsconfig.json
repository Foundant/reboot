{
	"compilerOptions": {
		"allowJs": false,
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"module": "commonjs",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"outDir": "./dist/js",
		"target": "es3"
	},
	"include": [
		"./src/**/*"
	]
}